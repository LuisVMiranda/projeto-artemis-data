	[12470] = {
		Title = "Vingança de Andrea I",
		Description = {
			"\t\t\tAndrea morreu pela foice de uma pequena criatura.\n\t\t\t\n\t\t\t^ff0000\"Não se engane pela sua aparência fofinha\"^000000\n\t\t\t\n\t\t\tVocê deve vingar a morte de Andrea.\n\t\t"
		},
