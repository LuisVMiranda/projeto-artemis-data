	},
	[12471] = {
		Title = "Vingança de Andrea II",
		Description = {
			"\t\t\tEnquanto agonizava, Andrea decidiu dedicar sua alma à purificação dos mortos.\n\t\t\t\n\t\t\t^ff0000\"É a alma de algum aventureiro que morreu anteriormente\"^000000\n\t\t\t\n\t\t\tNão se conhecia esse lado dedicado de Andrea.\n\t\t"
		},
		Summary = "Caçar Ghostring Caótico",
		RewardEXP = "1.750.000",
