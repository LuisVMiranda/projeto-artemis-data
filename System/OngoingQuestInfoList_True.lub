		RewardItemList = {
			{ ItemID = 25271, ItemNum = 1 }
		}
	},
	[12472] = {
		Title = "Vingança de Agnes I",
		Description = {
			"\t\t\tEnquanto sofria um grave ferimento, Agnes viu que esse inseto já perambulava sentindo o cheiro do seu sangue.\n\t\t\t\n\t\t\t^ff0099\"Sua cor é vermelha de tanto beber sangue\"^000000\n\t\t\t\n\t\t\tEla não teve uma boa experiência com esse inseto.\n\t\t"
		},
		Summary = "Caçar Mosca Caótica",
		RewardEXP = "1.750.000",
		RewardJEXP = "1.137.500",
		RewardItemList = {
